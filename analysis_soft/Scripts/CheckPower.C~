

//#include "NIMCrate.h"
//#include "NIMModule.h"
NIMCrate crate(10, 10, 3, 3, 1.5, 1.5);

void
CheckPower()
{
  NIMModule *ps794 = new NIMModule(0.42,0.40,0.10,0.160,0.040,0.065);
  ps794->SetName("ps794");
  NIMModule *ps755 = new NIMModule(0.25,0.40,0.00,0.165,0.035,0.060);
  ps755->SetName("ps755");
  NIMModule *ps744 = new NIMModule(0.40,0.45,0.05,0.165,0.000,0.080);
  ps744->SetName("ps744");

  NIMModule *n568b = new NIMModule(0.750,0,2.3,1.05,0,0);
  n568b->SetName("n568b");

  NIMModule *ps706 = new NIMModule(0.40,0.45,0.05,0.165,0,0.08);
  ps706->SetName("ps706");


  crate.AddModule(ps794,0);
  crate.AddModule(ps755,1);
  crate.AddModule(ps744,2);
  crate.AddModule(n568b,3);
  crate.AddModule(ps706,4);

  crate.PrintPowerUsage();
  crate.PrintAllModules();
  
}
