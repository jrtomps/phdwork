
#include <fstream>
#include <iostream>
#include "TFile.h"
using namespace std;

Float_t
DetermineDuration(int run)
{
  int start=0, stop=0;
  stringstream ss;
  ss << "run" << run << "_tree.root";
  TFile f(ss.str().data());
  RunInfo *rc = dynamic_cast<RunInfo*>(f.Get("StartInfo"));
  if (rc)
    {
      start = rc->GetTime();
    }

  rc = dynamic_cast<RunInfo*>(f.Get("EndInfo"));
  if (rc)
    {
      start = rc->GetTime();
    }

}

int
main()
{
  fstream file("trial.dat",fstream::in|fstream::out);
  if (!file.is_open())  return;

  while(1)
    {
      file >> run >> str >> dummy >> str;
      DetermineDuration(run);


    }

  return 0;
}

